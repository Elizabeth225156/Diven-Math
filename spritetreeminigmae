function preload(){
  treeSprite = loadImage("Tree Sprite.png");
  appleSprite = loadImage("Apple Sprite.png");
	banannaSprite = loadImage("Bananna Sprite.png")
}

function setup() {
  createCanvas(windowWidth, windowHeight);
}

function draw() {
  background(220);
	image(treeSprite, 0, 0);
	image(appleSprite, 0, 200);
}
